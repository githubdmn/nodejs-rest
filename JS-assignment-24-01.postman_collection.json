		{
			"name": "create-blog",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"",
							"const accessToken = pm.response.headers.get('access_token');",
							"if (accessToken) {",
							"    pm.environment.set('access_token', accessToken);",
							"    console.log('Access Token:', accessToken);",
							"} else {",
							"    console.error('Access Token not found in response headers.');",
							"}",
							"",
							"pm.test(\"Response status code is 201\", function () {",
							"  pm.expect(pm.response.to.have.status(201);",
							"});",
							"",
							"",
							"pm.test(\"Response has the required fields\", function () {",
							"    const responseData = pm.response.json();",
							"    ",
							"    pm.expect(responseData).to.be.an('object');",
							"    pm.expect(responseData).to.have.property('title');",
							"    pm.expect(responseData).to.have.property('text');",
							"    pm.expect(responseData).to.have.property('userId');",
							"    pm.expect(responseData).to.have.property('blogId');",
							"    pm.expect(responseData).to.have.property('id');",
							"});",
							"",
							"",
							"pm.test(\"Title is a non-empty string\", function () {",
							"  const responseData = pm.response.json();",
							"  ",
							"  pm.expect(responseData).to.be.an('object');",
							"  pm.expect(responseData.title).to.be.a('string').and.to.have.lengthOf.at.least(1, \"Title should not be empty\");",
							"});",
							"",
							"",
							"pm.test(\"User ID is a valid user identifier format\", function () {",
							"    const responseData = pm.response.json();",
							"    ",
							"    pm.expect(responseData.userId).to.match(/[a-f0-9]{24}/);",
							"});",
							"",
							"",
							"pm.test(\"Blog ID is a non-negative integer\", function () {",
							"  const responseData = pm.response.json();",
							"  ",
							"  pm.expect(responseData.blogId).to.be.a('number').and.to.be.at.least(0);",
							"});",
							"",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"",
							"const baseUrl = 'localhost:3000/api';",
							"",
							"console.log('API URL:', baseUrl);;",
							"",
							"const blogData = [",
							"    {",
							"        userId: 70972,",
							"        title: 'Blog Post 1',",
							"        content: 'Content for Blog Post 1.'",
							"    },",
							"    {",
							"        userId: 70972,",
							"        title: 'Blog Post 2',",
							"        content: 'Content for Blog Post 2.'",
							"    },",
							"    {",
							"        userId: 70972,",
							"        title: 'Blog Post 3',",
							"        content: 'Content for Blog Post 3.'",
							"    },",
							"    {",
							"        userId: 70972,",
							"        title: 'Blog Post 4',",
							"        content: 'Content for Blog Post 4.'",
							"    },",
							"    {",
							"        userId: 70972,",
							"        title: 'Blog Post 5',",
							"        content: 'Content for Blog Post 5.'",
							"    }",
							"];",
							"",
							"blogData.forEach((blog, index) => {",
							"    pm.sendRequest({",
							"        url: `${baseUrl}/api/blogs`,",
							"        method: 'POST',",
							"        header: {",
							"            'Content-Type': 'application/json',",
							"        },",
							"        body: {",
							"            mode: 'raw',",
							"            raw: JSON.stringify(blog),",
							"        },",
							"        callback: function (error, response) {",
							"            if (error) {",
							"                console.error(`Error creating Blog Post ${index + 1}:`, error);",
							"            } else {",
							"                console.log(`Blog Post ${index + 1} created successfully. Response:`, response.text());",
							"            }",
							"        }",
							"    });",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
